<h1>Listing cutes</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cutes.each do |cute| %>
      <tr>
        <td><%= cute.url %></td>
        <td><%= link_to 'Show', cute %></td>
        <td><%= link_to 'Edit', edit_cute_path(cute) %></td>
        <td><%= link_to 'Destroy', cute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cute', new_cute_path %>
